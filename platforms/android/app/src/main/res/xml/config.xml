<?xml version='1.0' encoding='utf-8'?>
<widget id="io.voiceit.voiceitapi2cordovademo" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="NativeStorage">
        <param name="android-package" value="NativeStorage" />
    </feature>
    <feature name="CDVOrientation">
        <param name="android-package" value="cordova.plugins.screenorientation.CDVOrientation" />
    </feature>
    <feature name="VoiceItCordova">
        <param name="android-package" value="com.voiceittech.plugins.VoiceItCordova" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>VoiceIt API 2 Cordova Demo App</name>
    <description>
        A sample Apache Cordova application that shows the integration of cordova-plugin-voiceit-api-2
    </description>
    <author email="armaan@voiceit.io" href="http://voiceit.io">
        VoiceIt Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/icon.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
